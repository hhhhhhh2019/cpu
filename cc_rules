#terminals

HEX_NUMBER (0x|0X)[0-9a-fA-f]+
BIN_NUMBER (0b|0B)[0-1]+
DEC_NUMBER [0-9]+
NEWLINE \n
COMMA \,
POINT \.
COLON \:
SEMICOLON \;
STRING ".*?"(?<!\\")
CHARACTER '.'

LBR \(
RBR \)
LSBR \[
RSBR \]
LCBR \{
RCBR \}

INC \+\+
DEC \-\-

ARROW \->

ASSIGN_PLUS \+=
ASSIGN_MINUS \-=
ASSIGN_STAR \*=
ASSIGN_SLASH \/=
ASSIGN_MOD \%=
ASSIGN_LEFT_SHIFT <<=
ASSIGN_RIGHT_SHIFT >>=
ASSIGN_AND \&=
ASSIGN_OR \|=
ASSIGN_XOR \^=

LEFT_SHIFT <<
RIGHT_SHIFT >>

PLUS \+
MINUS \-
STAR \*
SLASH \/
PERCENT \%

DOUBLE_AMPERSAND \&\&
DOUBLE_PIPE \|\|

AMPERSAND \&
PIPE \|
TILDA \~
CARET \^
EXCLAMATION \!

ASSIGN =
EQUALS ==
NOT_EQUALS \!=
